<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="esprit.tn.pi_kavafx.controllers.ClientResultsController"
      spacing="16">

    <padding>
        <Insets top="25" right="25" bottom="25" left="25"/>
    </padding>

    <Label text="Mes Résultats" styleClass="page-title"/>
    <Label text="Historique de vos quiz passés" styleClass="page-subtitle"/>

    <TableView fx:id="table" VBox.vgrow="ALWAYS">

        <columns>
            <TableColumn fx:id="colFormation" text="Formation" prefWidth="250"/>
            <TableColumn fx:id="colScore" text="Score" prefWidth="120"/>
            <TableColumn fx:id="colTotal" text="Total" prefWidth="120"/>
            <TableColumn fx:id="colDate" text="Date" prefWidth="200"/>
        </columns>

    </TableView>

</VBox>
