<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.formation_quiz_result.ClientResultsController"
      spacing="14">

    <padding>
        <Insets top="22" right="22" bottom="22" left="22"/>
    </padding>

    <Label text="Mes Résultats" styleClass="page-title"/>
    <Label text="Historique de vos quiz passés" styleClass="page-subtitle"/>

    <ScrollPane fitToWidth="true"
                style="-fx-background-color:transparent;"
                VBox.vgrow="ALWAYS">

        <content>
            <FlowPane fx:id="cardsFlow"
                      hgap="14"
                      vgap="14"
                      prefWrapLength="980"
                      style="-fx-background-color:transparent;"/>
        </content>

    </ScrollPane>

</VBox>
